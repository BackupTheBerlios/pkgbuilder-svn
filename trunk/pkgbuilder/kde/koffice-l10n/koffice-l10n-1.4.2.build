inherit kde_i18n

PKG_BUILD="1"

unset PKG_URL
for file in $PKG_FILE_NAME ; do
    PKG_URL="$PKG_URL $KDE_BASE_URL/koffice-$PKG_VERSION/src/$PKG_NAME/$file"
done

PKG_USE=""

PKG_DEPENDS="=kde/koffice-1.4.1"

PKG_DOC_FILES="ChangeLog COPYING INSTALL README"

