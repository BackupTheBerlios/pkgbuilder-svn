do_fetch() {
    cd $TMP
    
    echo "$PKG_CVSPASSWD" | cvs -d $PKG_CVSROOT login &&    
    cvs -d $PKG_CVSROOT co -d $PKG_NAME-$PKG_VERSION $PKG_MODULE
    
    return $?
}
